{
  "name": "onepiece-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "pwa": "pwa-assets-generator --preset minimal public/vite.svg",
    "lint:style": "stylelint '**/*.style.ts' --fix",
